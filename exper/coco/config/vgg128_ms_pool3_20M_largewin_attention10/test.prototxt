############### scale features #############
layer {
  bottom: "fc_ms_fusion"
  bottom: "attention1"
  top: "fc8_product"
  name: "fc8_product"
  type: "SpatialProduct"
}
layer {
  bottom: "fc_ms_fusion_res075_interp"
  bottom: "attention2"
  top: "fc8_res075_product" 
  name: "fc8_res075_product"
  type: "SpatialProduct"
}
layer {
  bottom: "fc_ms_fusion_res05_interp"
  bottom: "attention3"
  top: "fc8_res05_product" 
  name: "fc8_res05_product"
  type: "SpatialProduct"
}
### add features ###
layer {
  bottom: "fc8_product"
  bottom: "fc8_res075_product"
  bottom: "fc8_res05_product"
  top: "fc8_fusion"
  name: "fc8_fusion"
  type: "Eltwise"
  eltwise_param {
    operation: SUM
  }
}

### upsampling ###
layer {
  bottom: "fc8_fusion"
  top: "fc8_fusion_interp"
  name: "fc8_fusion_interp"
  type: "Interp"
  interp_param {
    zoom_factor: 8
  }
}

### save results ###
layer {
  bottom: "fc8_fusion_interp"
  top: "fc8_fusion_argmax"
  name: "fc8_fusion_argmax"
  type: "ArgMax"
}

layer {
  name: "fc8_mat"
  type: "MatWrite"
  bottom: "fc8_fusion_argmax"
  mat_write_param {
    prefix: "${FEATURE_DIR}/${TEST_SET}/fc8/"
    source: "${EXP}/list/${TEST_SET}_id.txt"
    strip: 0
    period: 1
  }
  include: { phase: TEST }
}
